<mapping>

    <!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
    <!-- %                                                     % -->
    <!-- %  xmlns:oval-definitions-5                           % -->
    <!-- %                                                     % -->
    <!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->

    <class name="jp.go.aist.six.oval.model.v5.definitions.OvalDefinitions"
        extends="jp.go.aist.six.oval.model.v5.OvalDocument">
        <map-to xml="oval_definitions"
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"
            ns-prefix=""/>

        <field name="generator" 
            type="jp.go.aist.six.oval.model.v5.common.GeneratorType">
            <bind-xml name="generator" node="element"/>
        </field>

<!--
        <field name="definitions" 
            type="jp.go.aist.six.oval.model.v5.definitions.DefinitionType"
            collection="collection">
            <bind-xml name="definition" node="element" location="definitions"/>
        </field>
-->
        <field name="definitions" 
            type="jp.go.aist.six.oval.model.v5.definitions.DefinitionsType">
            <bind-xml name="definitions" node="element"/>
        </field>

        <field name="tests" 
            type="jp.go.aist.six.oval.model.v5.definitions.TestsType">
            <bind-xml name="tests" node="element" />
        </field>


        <field name="objects" 
            type="jp.go.aist.six.oval.model.v5.definitions.SystemObjectsType">
            <bind-xml name="objects" node="element"/>
        </field>

        <field name="states" 
            type="jp.go.aist.six.oval.model.v5.definitions.StatesType">
            <bind-xml name="states" node="element"/>
        </field>

        <field name="variables" 
            type="jp.go.aist.six.oval.model.v5.definitions.VariablesType">
            <bind-xml name="variables" node="element"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.DefinitionsType">
        <map-to 
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"
            ns-prefix=""/>

        <field name="definition" 
            type="jp.go.aist.six.oval.model.v5.definitions.DefinitionType"
            collection="collection">
            <bind-xml name="definition" node="element"/>
        </field>
    </class>



    <!-- definition -->
    <class name="jp.go.aist.six.oval.model.v5.definitions.DefinitionType"
        extends="jp.go.aist.six.oval.model.v5.OvalEntity">
        <map-to xml="definition" 
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"
            ns-prefix=""/>

        <field name="metadata" 
            type="jp.go.aist.six.oval.model.v5.definitions.MetadataType">
            <bind-xml name="metadata" node="element"/>
        </field>

        <field name="notes" 
            type="jp.go.aist.six.oval.model.v5.definitions.NotesType">
            <bind-xml name="notes" node="element"/>
        </field>

        <field name="criteria" 
            type="jp.go.aist.six.oval.model.v5.definitions.CriteriaType">
            <bind-xml name="criteria" node="element"/>
        </field>

        <field name="definitionClass" 
            type="string"
            handler="jp.go.aist.six.oval.core.xml.v5.common.ClassEnumerationHandler">
            <!-- type="jp.go.aist.six.oval.model.v5.common.ClassEnumeration" -->
            <bind-xml name="class" node="attribute"/>
        </field>
    </class>


    <!-- metadata -->
    <class name="jp.go.aist.six.oval.model.v5.definitions.MetadataType">
        <map-to 
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"
            ns-prefix=""/>

        <field name="title" type="string">
            <bind-xml name="title" node="element"/>
        </field>

        <field name="affected" 
            type="jp.go.aist.six.oval.model.v5.definitions.AffectedType"
            collection="collection">
            <bind-xml name="affected" node="element"/>
        </field>

        <field name="reference" 
            type="jp.go.aist.six.oval.model.v5.definitions.ReferenceType"
            collection="collection">
            <bind-xml name="reference" node="element"/>
        </field>

        <field name="description" type="string">
            <bind-xml name="description" node="element"/>
        </field>

        <field name="additionalMetadata" 
            type="jp.go.aist.six.oval.model.v5.definitions.MetadataItem"
            collection="collection">
            <bind-xml auto-naming="deriveByClass" node="element"/>
        </field>
    </class>

    <class name ="jp.go.aist.six.oval.model.v5.definitions.MetadataItem"
        verify-constructable="false">
    </class>



    <!-- affected -->
    <class name="jp.go.aist.six.oval.model.v5.definitions.AffectedType">
        <map-to 
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"/>

        <field name="family" 
            type="string"
            handler="jp.go.aist.six.oval.core.xml.v5.common.FamilyEnumerationHandler">
            <!-- type="jp.go.aist.six.oval.model.v5.common.FamilyEnumeration" -->
            <bind-xml name="family" node="attribute"/>
        </field>

        <field name="platform" 
            type="jp.go.aist.six.oval.model.v5.definitions.Platform"
            collection="collection"
            get-method="iteratePlatform"
            set-method="addPlatform"
            >
            <bind-xml name="platform" node="element"/>
        </field>

        <field name="product" 
            type="jp.go.aist.six.oval.model.v5.definitions.Product"
            collection="collection"
            get-method="iterateProduct"
            set-method="addProduct"
            >
            <bind-xml name="product" node="element"/>
        </field>
    </class>

    <class name="jp.go.aist.six.oval.model.v5.definitions.Platform">
        <map-to xml="platform"
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"/>

        <field name="name" type="string">
            <bind-xml node="text"/>
        </field>
    </class>

    <class name="jp.go.aist.six.oval.model.v5.definitions.Product">
        <map-to xml="product"
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"/>

        <field name="name" type="string">
            <bind-xml node="text"/>
        </field>
    </class>



    <!-- reference -->
    <class name="jp.go.aist.six.oval.model.v5.definitions.ReferenceType">
        <map-to 
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"/>

        <field name="source" type="string">
            <bind-xml name="source" node="attribute"/>
        </field>

        <field name="refID" type="string">
            <bind-xml name="ref_id" node="attribute"/>
        </field>

        <field name="refURL" type="string">
            <bind-xml name="ref_url" node="attribute"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.NotesType">
        <map-to xml="notes"
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"
            ns-prefix=""/>

        <field name="note" 
            type="jp.go.aist.six.oval.model.v5.definitions.Note"
            collection="collection">
            <bind-xml name="note" node="element"/>
        </field>
    </class>

    <class name="jp.go.aist.six.oval.model.v5.definitions.Note">
        <map-to xml="note"
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"
            ns-prefix=""/>

        <field name="content" type="string">
            <bind-xml node="text"/>
        </field>
    </class>



    <class name ="jp.go.aist.six.oval.model.v5.definitions.CriteriaElement"
        verify-constructable="false">

        <field name="negate" type="boolean">
            <bind-xml name="negate" node="attribute"/>
        </field>

        <field name="comment" type="string">
            <bind-xml name="comment" node="attribute"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.CriteriaType"
        extends="jp.go.aist.six.oval.model.v5.definitions.CriteriaElement">
        <map-to xml="criteria"
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"/>

        <field name="operator" 
            type="string"
            handler="jp.go.aist.six.oval.core.xml.v5.common.OperatorEnumerationHandler">
            <!-- type="jp.go.aist.six.oval.model.v5.common.OperatorEnumeration" -->
            <bind-xml name="operator" node="attribute"/>
        </field>

        <field name="elements" 
            type="jp.go.aist.six.oval.model.v5.definitions.CriteriaElement"
            collection="collection">
            <bind-xml auto-naming="deriveByClass" node="element"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.CriterionType"
        extends="jp.go.aist.six.oval.model.v5.definitions.CriteriaElement">
        <map-to xml="criterion"
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"/>

        <field name="testRef" type="string">
            <bind-xml name="test_ref" node="attribute"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.ExtendDefinitionType"
        extends="jp.go.aist.six.oval.model.v5.definitions.CriteriaElement">
        <map-to xml="extend_definition"
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"/>

        <field name="definitionRef" type="string">
            <bind-xml name="definition_ref" node="attribute"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.TestsType">
        <field name="test" 
            type="jp.go.aist.six.oval.model.v5.definitions.TestType"
            collection="collection">
            <bind-xml auto-naming="deriveByClass" node="element"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.TestType"
        extends="jp.go.aist.six.oval.model.v5.CommentedOvalEntity"
        >
<!--
        <map-to
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"
            ns-prefix=""/>
        verify-constructable="false"
-->

        <field name="notes" 
            type="jp.go.aist.six.oval.model.v5.definitions.NotesType">
            <bind-xml name="notes" node="element"/>
        </field>
<!--
        <field name="notes" 
            type="jp.go.aist.six.oval.model.v5.definitions.NotesType">
            <bind-xml name="notes" node="element"
                xmlns="http://oval.mitre.org/XMLSchema/oval-definitions-5"/>
        </field>
-->

        <field name="checkExistence"
            type="string"
            handler="jp.go.aist.six.oval.core.xml.v5.common.ExistenceEnumerationHandler">
            <!-- type="jp.go.aist.six.oval.model.v5.common.ExistenceEnumeration" -->
            <bind-xml name="check_existence" node="attribute"/>
        </field>

        <field name="check" 
            type="string"
            handler="jp.go.aist.six.oval.core.xml.v5.common.CheckEnumerationHandler">
            <!-- type="jp.go.aist.six.oval.model.v5.common.CheckEnumeration" -->
            <bind-xml name="check" node="attribute"/>
        </field>

        <field name="stateOperator" 
            type="string"
            handler="jp.go.aist.six.oval.core.xml.v5.common.OperatorEnumerationHandler">
            <!-- type="jp.go.aist.six.oval.model.v5.common.OperatorEnumeration" -->
            <bind-xml name="state_operator" node="attribute"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.SystemObjectRefType">
        <field name="objectRef" type="string">
            <bind-xml name="object_ref" node="attribute"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.StateRefType">
        <field name="stateRef" type="string">
            <bind-xml name="state_ref" node="attribute"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.SystemObjectsType">
        <field name="object" 
            type="jp.go.aist.six.oval.model.v5.definitions.SystemObjectType"
            collection="collection">
