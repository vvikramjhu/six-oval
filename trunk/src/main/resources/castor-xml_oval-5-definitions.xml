        <field name="notes" 
            type="jp.go.aist.six.oval.model.v5.definitions.NotesType">
            <bind-xml name="notes" node="element"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.Filter">
        <map-to 
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"
            ns-prefix=""/>

        <field name="content" type="string">
            <bind-xml node="text"/>
        </field>

        <field name="action" 
            type="string"
            handler="jp.go.aist.six.oval.core.xml.v5.definitions.FilterActionEnumerationHandler">
            <!-- type="jp.go.aist.six.oval.model.v5.definitions.FilterActionEnumeration" -->
            <bind-xml name="action" node="attribute"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.StatesType">
        <field name="state" 
            type="jp.go.aist.six.oval.model.v5.definitions.StateType"
            collection="collection">
            <bind-xml auto-naming="deriveByClass" node="element"/>
        </field>
    </class>



    <class name="jp.go.aist.six.oval.model.v5.definitions.StateType"
        extends="jp.go.aist.six.oval.model.v5.CommentedOvalEntity"
