    <class name="jp.go.aist.six.oval.model.v5.mitre.OvalRepository"
        extends="jp.go.aist.six.oval.model.v5.definitions.MetadataItem">
        <map-to xml="oval_repository"
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5"/>

        <field name="status" 
            type="string"
            handler="jp.go.aist.six.oval.core.xml.v5.mitre.DefinitionStatusEnumerationHandler">
            <!-- type="jp.go.aist.six.oval.model.v5.mitre.DefinitionStatusEnumeration" -->
            <bind-xml name="status" node="element"/>
        </field>

        <field name="dates" 
            type="jp.go.aist.six.oval.model.v5.mitre.Event"
            collection="collection">
