        <map-to xml="file_test"
                xmlns:oval-sc="http://oval.mitre.org/XMLSchema/oval-system-characteristics-5"
            ns-uri="http://oval.mitre.org/XMLSchema/oval-definitions-5#unix"
            ns-prefix=""/>
